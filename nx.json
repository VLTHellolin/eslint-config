{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/src/**/*"
    ],
    "production": [
      "default",
      "!{projectRoot}/src/**/*.test.{ts,tsx,js,jsx}"
    ]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "inputs": [
        "production",
        "^production"
      ],
      "outputs": [
        "{projectRoot}/dist/**"
      ]
    }
  },
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ],
  "nxCloudId": "698767c3a554a970c7d8db46"
}